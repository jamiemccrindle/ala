<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:ala="http://www.niccstandards.org.uk/ala_1.0.xsd"
           targetNamespace="http://www.niccstandards.org.uk/ala_1.0.xsd"
           elementFormDefault="qualified">

  <xs:annotation>
    <xs:documentation>
      Schema description for the NICC Active Line Access B2B protocol
    </xs:documentation>
  </xs:annotation>

  <!-- top level elements -->

  <xs:element name="orderRequest">
    <xs:annotation>
      <xs:documentation>
        Order a new service, orders can be migrations, new installs, modifies or ceases
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="orderReferences" type="ala:orderReferences" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="buyerContactDetails" type="ala:buyerContactDetails" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
        <xs:choice maxOccurs="1">
          <xs:element name="alaMigration" type="ala:alaMigration" />
          <xs:element name="alaNewInstall" type="ala:alaNewInstall" />
          <xs:element name="alaModify" type="ala:alaModify" />
          <xs:element name="alaCease" type="ala:alaCease" />
        </xs:choice>
        <xs:element maxOccurs="1" name="notes" type="ala:notes" />
        <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:element name="orderResponse"/>

  <xs:element name="notifyOfOrderStatus" type="ala:notifyOfOrderStatus"/>

  <xs:complexType name="notifyOfOrderStatus">
    <xs:annotation>
      <xs:documentation>
        Messages sent back to the ALA user to indicate progress of an order. To facilitate
        correct construction of messages, these messages will either contain an alaOrderInProgress
        message if the order is in progress, alaOrderFailed if the order has failed and
        alaOrderCompleted if the order has completed.
      </xs:documentation>
    </xs:annotation>
    <xs:choice maxOccurs="unbounded">
      <xs:element maxOccurs="1" name="message" type="ala:message" />
      <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
      <xs:element maxOccurs="1" name="seller" type="ala:seller" />
      <xs:element maxOccurs="1" name="orderReferences" type="ala:orderReferences" />
      <xs:element maxOccurs="1" name="sequenceNumber" type="ala:sequenceNumber" />
      <xs:element maxOccurs="1" name="issuedAt" type="ala:issuedAt" />
      <xs:element maxOccurs="1" name="slaClock" type="ala:slaClock" />
      <xs:element maxOccurs="1" name="alaOrderStatus" type="ala:alaOrderStatus" />

      <xs:choice maxOccurs="1">
        <xs:element name="alaOrderInProgress" type="ala:alaOrderInProgress" />
        <xs:element name="alaOrderFailed" type="ala:alaOrderFailed" />
        <xs:element name="alaOrderCompleted" type="ala:alaOrderCompleted" />
      </xs:choice>

      <xs:element maxOccurs="1" name="notes" type="ala:notes" />
      <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
    </xs:choice>
  </xs:complexType>

  <xs:element name="notifyOfOrderStatusResponse"/>

  <xs:element name="amendOrderRequest">
    <xs:annotation>
      <xs:documentation>
        Request to amend an in flight order.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="orderReferences" type="ala:orderReferences" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="buyerContactDetails" type="ala:buyerContactDetails" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
        <xs:element maxOccurs="1" name="targetSellerOrderReference" type="ala:sellerOrderReference" />
        <xs:element maxOccurs="1" name="acceptCharges" type="ala:acceptCharges" />
        <xs:element maxOccurs="1" name="notes" type="ala:notes" />
        <xs:element maxOccurs="1" name="appointment" type="ala:appointment" />
        <xs:element maxOccurs="1" name="requestedDate" type="ala:requestedDate" />
        <xs:element maxOccurs="1" name="serviceItem" type="ala:serviceItem" />
        <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:element name="amendOrderResponse">
    <xs:annotation>
      <xs:documentation>
        An ALA Provider can either respond that the amendment is pending or can reject the amendment up front.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
        <xs:element maxOccurs="1" name="orderReferences" type="ala:orderReferences" />
        <xs:choice maxOccurs="1">
          <xs:element name="amendmentPending" type="ala:amendmentPending" />
          <xs:element name="amendmentRejected" type="ala:amendmentRejected" />
        </xs:choice>
        <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:element name="cancelOrderRequest">
    <xs:annotation>
      <xs:documentation>
        Request to cancel an in flight order
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
        <xs:element maxOccurs="1" name="orderReferences" type="ala:orderReferences" />
        <xs:element maxOccurs="1" name="targetSellerOrderReference" type="ala:sellerOrderReference" />
        <xs:element maxOccurs="1" name="cancellationReason" type="ala:cancellationReason" />
        <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:element name="cancelOrderResponse">
    <xs:annotation>
      <xs:documentation>
        The ALA Provider can either respond that the cancellation is pending or reject it up front.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
        <xs:element maxOccurs="1" name="orderReferences" type="ala:orderReferences" />
        <xs:choice maxOccurs="1">
          <xs:element name="cancellationPending" type="ala:cancellationPending" />
          <xs:element name="cancellationRejected" type="ala:cancellationRejected" />
        </xs:choice>
        <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <!-- unsolicited cease -->

  <xs:element name="unsolicitedCeaseRequest" type="ala:unsolicitedCeaseRequest"/>

  <xs:complexType name="unsolicitedCeaseRequest">
    <xs:annotation>
      <xs:documentation>
        Unsolicited ceases are sent by the ALA Provider to the ALA User to indicate that a service
        is being ceased.
      </xs:documentation>
    </xs:annotation>
    <xs:choice maxOccurs="unbounded">
      <xs:element maxOccurs="1" name="message" type="ala:message" />
      <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
      <xs:element maxOccurs="1" name="seller" type="ala:seller" />
      <xs:element maxOccurs="1" name="orderReferences" type="ala:orderReferences" />
      <xs:element maxOccurs="1" name="serviceIdentifier" type="ala:serviceIdentifier" />
      <xs:element maxOccurs="1" name="requestedDate" type="ala:requestedDate" />
      <xs:element maxOccurs="1" name="notes" type="ala:notes" />
      <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
    </xs:choice>
  </xs:complexType>

  <xs:element name="unsolicitedCeaseResponse"/>

  <!-- mac request -->

  <xs:element name="macRequest">
    <xs:annotation>
      <xs:documentation>
        Request a MAC
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
        <xs:element maxOccurs="1" name="serviceIdentifiers" type="ala:serviceIdentifiers" />
        <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:element name="macResponse">
    <xs:annotation>
      <xs:documentation>
        An ALA Provider can either return a MAC or reject the request
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
        <xs:choice maxOccurs="1">
          <xs:element maxOccurs="1" name="macAccepted" type="ala:macAccepted" />
          <xs:element maxOccurs="1" name="macRejected" type="ala:dialogueServiceRejected" />
        </xs:choice>
        <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:complexType name="macAccepted">
    <xs:choice maxOccurs="unbounded">
      <xs:element maxOccurs="1" name="mac" type="ala:mac" />
      <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
    </xs:choice>
  </xs:complexType>

  <!-- query installation details -->

  <xs:element name="queryInstallationDetailsRequest">
    <xs:annotation>
      <xs:documentation>
        Query the current configuration of an active service
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
        <xs:element maxOccurs="1" name="serviceIdentifier" type="ala:serviceIdentifier" />
        <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:element name="queryInstallationDetailsResponse">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
        <xs:element maxOccurs="1" name="queryInstallationDetailsAccepted" type="ala:queryInstallationDetailsAccepted" />
        <xs:element maxOccurs="1" name="queryInstallationDetailsRejected" type="ala:dialogueServiceRejected" />
        <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:complexType name="queryInstallationDetailsAccepted">
    <xs:choice maxOccurs="unbounded">
      <xs:element maxOccurs="1" name="activeService" type="ala:activeService" />
      <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
    </xs:choice>
  </xs:complexType>

  <!-- query line availability -->

  <xs:element name="queryLineAvailabilityRequest">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
        <xs:element maxOccurs="1" name="addressKey" type="ala:addressKey" />
        <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:element name="queryLineAvailabilityResponse">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
        <xs:element maxOccurs="1" name="queryLineAvailabilityAccepted" type="ala:queryLineAvailabilityAccepted" />
        <xs:element maxOccurs="1" name="queryLineAvailabilityRejected" type="ala:dialogueServiceRejected" />
        <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:complexType name="queryLineAvailabilityAccepted">
    <xs:choice maxOccurs="unbounded">
      <xs:element maxOccurs="1" name="activeServices" type="ala:activeServices" />
      <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
    </xs:choice>
  </xs:complexType>

  <!-- query order status -->

  <xs:element name="queryOrderStatusRequest">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
        <xs:element maxOccurs="1" name="buyerOrderReferences" type="ala:buyerOrderReferences" />
        <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:element name="queryOrderStatusResponse">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
        <xs:choice maxOccurs="1">
          <xs:element maxOccurs="1" name="queryOrderStatusAccepted" type="ala:queryOrderStatusAccepted" />
          <xs:element maxOccurs="1" name="queryOrderStatusRejected" type="ala:dialogueServiceRejected" />
        </xs:choice>
        <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:complexType name="queryOrderStatusAccepted">
    <xs:choice maxOccurs="unbounded">
      <xs:element maxOccurs="1" name="orderStatuses" type="ala:orderStatuses" />
      <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
    </xs:choice>
  </xs:complexType>


  <!-- validate mac request -->

  <xs:element name="validateMacRequest">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
        <xs:element maxOccurs="1" name="mac" type="ala:mac" />
        <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:element name="validateMacResponse">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
        <xs:choice maxOccurs="1">
          <xs:element maxOccurs="1" name="validateMacAccepted" type="ala:validateMacAccepted" />
          <xs:element maxOccurs="1" name="validateMacRejected" type="ala:dialogueServiceRejected" />
        </xs:choice>
        <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:complexType name="validateMacAccepted">
    <xs:choice maxOccurs="unbounded">
      <xs:element maxOccurs="1" name="products" type="ala:products" />
      <xs:element maxOccurs="1" name="location" type="ala:location" />
      <xs:element name="macStatus">
        <xs:complexType>
          <xs:choice maxOccurs="1">
            <xs:element name="valid">
              <xs:complexType />
            </xs:element>
            <xs:element name="invalid">
              <xs:complexType />
            </xs:element>
            <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
    </xs:choice>
  </xs:complexType>

  <!-- network availability -->

  <xs:element name="queryProductsForLocationRequest">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
        <xs:element maxOccurs="1" name="telephoneNumber" type="ala:telephoneNumber" />
        <xs:element maxOccurs="1" name="location" type="ala:location" />
        <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:element name="queryProductsForLocationResponse">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
        <xs:choice maxOccurs="1">
          <xs:element maxOccurs="1" name="queryProductsForLocationAccepted" type="ala:queryProductsForLocationAccepted" />
          <xs:element maxOccurs="1" name="queryProductsForLocationRejected" type="ala:dialogueServiceRejected" />
        </xs:choice>
        <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:complexType name="queryProductsForLocationAccepted">
    <xs:choice maxOccurs="unbounded">
      <xs:element maxOccurs="1" name="availableProducts" type="ala:availableProducts" />
      <xs:element maxOccurs="1" name="locations" type="ala:locations" />
      <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
    </xs:choice>
  </xs:complexType>

  <!-- address match -->

  <xs:element name="queryAddressSearchRequest">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
        <xs:element maxOccurs="1" name="location" type="ala:location" />
        <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:element name="queryAddressSearchResponse">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
        <xs:choice maxOccurs="1">
          <xs:element maxOccurs="1" name="queryAddressSearchAccepted" type="ala:queryAddressSearchAccepted" />
          <xs:element maxOccurs="1" name="queryAddressSearchRejected" type="ala:dialogueServiceRejected" />
        </xs:choice>
        <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:complexType name="queryAddressSearchAccepted">
    <xs:choice maxOccurs="unbounded">
      <xs:element name="addressKeyType" type="ala:addressKeyType"/>
      <xs:element maxOccurs="1" name="locations" type="ala:locations" />
      <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
    </xs:choice>
  </xs:complexType>

  <!-- appointment -->

  <xs:element name="queryAppointmentAvailabilityRequest">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
        <xs:element maxOccurs="1" name="addressKey" type="ala:addressKey" />
        <xs:element maxOccurs="1" name="appointmentsFrom" type="ala:appointmentsFrom" />
        <xs:element maxOccurs="1" name="appointmentType" type="ala:appointmentType"/>
        <xs:element maxOccurs="1" name="technology" type="ala:technology" />
        <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:element name="queryAppointmentAvailabilityResponse">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
        <xs:choice maxOccurs="1">
          <xs:element maxOccurs="1" name="queryAppointmentAvailabilityAccepted" type="ala:queryAppointmentAvailabilityAccepted" />
          <xs:element maxOccurs="1" name="queryAppointmentAvailabilityRejected" type="ala:dialogueServiceRejected" />
        </xs:choice>
        <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:complexType name="queryAppointmentAvailabilityAccepted">
    <xs:choice maxOccurs="unbounded">
      <xs:element maxOccurs="1" name="appointmentSlots" type="ala:appointmentSlots" />
      <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
    </xs:choice>
  </xs:complexType>

  <!-- reserve appointment -->

  <xs:element name="reserveAppointmentRequest">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
        <xs:element maxOccurs="1" name="appointmentSlot" type="ala:appointmentSlot" />
        <xs:element maxOccurs="1" name="appointmentType" type="ala:appointmentType" />
        <xs:element maxOccurs="1" name="technology" type="ala:technology" />
        <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
      </xs:choice>
    </xs:complexType>
  </xs:element>

  <xs:element name="reserveAppointmentResponse">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="1" name="message" type="ala:message" />
        <xs:element maxOccurs="1" name="buyer" type="ala:buyer" />
        <xs:element maxOccurs="1" name="seller" type="ala:seller" />
